local SpeedUpBtn = makeButton(PageSpeed, "MAIS RÁPIDO (+)", 65, Color3.fromRGB(0, 100, 50))
local SpeedDownBtn = makeButton(PageSpeed, "MAIS LENTO (-)", 120, Color3.fromRGB(100, 0, 0))
